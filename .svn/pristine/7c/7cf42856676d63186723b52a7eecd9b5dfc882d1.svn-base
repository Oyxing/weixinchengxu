<!--pages/game/game.wxml-->
<component-topnav inner-title="开启娱乐模式" to-home="false" is-show-back="true"></component-topnav> 
<scroll-view scroll-y class="box" style="height:calc(100vh - {{statusBarHeight}}px - {{titleBarHeight}}px)">
  <canvas canvas-id="myCanvas" style="border: 1px solid;" bindtouchend="bindtouchend" bindtouchstart='bindtouchstart' style="width:100%;height:100%;"/>
  <button class="bindstart" bindtap='bindstart'>开始</button>
</scroll-view>
