<!--pages/feedback/feedback.wxml-->
<component-topnav inner-title="添加评论" to-home="false" is-show-back="true"></component-topnav> 
<scroll-view class="box">
  <form bindsubmit="formSubmit" bindreset="formReset">
    <view class="section">
      <textarea name="feedback" placeholder="添加评论" auto-focus />
    </view>
    <view class="submit">
      <button form-type="submit" disabled="{{btndisabled}}">提 交 评 论</button>
    </view>
  </form>
</scroll-view>