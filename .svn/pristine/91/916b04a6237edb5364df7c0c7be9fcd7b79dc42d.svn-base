/**index.wxss**/
page{
  padding: 0px;
  margin: 0px;
}
.navbox {
  position: fixed;
  background: #fff;
  width: 100%;
  height: 140rpx;
  bottom: 0rpx;
  border-top-style: solid;
  border-top-color: #d4d4d4;
  border-top-width: 1rpx;
}
.navbox view {
  float: left;
  width: 33%;
  height: 100%;
  text-align: center;
}
.navbox view label:nth-child(1){
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  background-size: 100%;
  margin-top: 20rpx;
}
.navbox view label:nth-child(2){
   display: inline-block;
  width: 100%;
  font-size: 30rpx;
  color: #ccc;
}