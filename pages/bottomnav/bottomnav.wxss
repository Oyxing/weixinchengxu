/* pages/bottomnav/bottomnav.wxss */
.navbox {
  position: fixed;
  width: 100%;
  height: 70px;
  bottom: 0px;
}
.navbox view {
  float: left;
  width: 33%;
  height: 100%;
  text-align: center;
}
.navbox view label:nth-child(1){
  display: inline-block;
  width: 32px;
  height: 32px;
}
.navbox view label:nth-child(2){
   display: inline-block;
  width: 100%;
  font-size: 15px;
  color: #ccc;
}
