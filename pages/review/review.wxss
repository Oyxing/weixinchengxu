/* pages/feedback/feedback.wxss */
.box{
  position: relative;
  width: 100%;
  height: 100vh;
  background: #f3f2f7; 
}
form .section{
  width: 100%;
  margin-top: 20px;
  height: 400rpx;
  background: #fff;
  font-size: 30rpx;
}
form .section textarea{
  width: 90%;
  padding: 20rpx;  
  height: 90%;
}

form .contact{
  margin-top: 20rpx;
}
form .contact .section__title{
  font-size: 30rpx;
  color: #ccc;
  padding: 10rpx 20rpx;
}
form .contact input{
  padding: 20rpx 0rpx;
  background: #fff;
} 
form .submit button{
  width: 90%;
  margin-left: 5%;
  margin-top: 20rpx;
  border-radius: 50rpx;
  background: #ff9632;
  color: #fff;
}